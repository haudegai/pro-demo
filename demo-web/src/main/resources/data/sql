-- ----------------------------
-- Table structure for pump_trouble_device
-- ----------------------------
DROP TABLE IF EXISTS `pump_trouble_device`;
CREATE TABLE `pump_trouble_device` (
  `station_code` varchar(50) NOT NULL COMMENT '学习编码',
  `device_id` varchar(50) NOT NULL COMMENT '设备id',
  `ticket_code` varchar(50) NOT NULL COMMENT '故障编码',
  `trouble_describe` varchar(2000) DEFAULT NULL COMMENT '故障描述',
  `status` tinyint(4) DEFAULT '0' COMMENT '任务设备维修状态：0未维修，1已维修'
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='故障设备关系（设备视图）';

