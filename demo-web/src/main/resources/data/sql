  `id` bigint(19) NOT NULL AUTO_INCREMENT COMMENT '序号',
  `code` varchar(50) NOT NULL DEFAULT '' COMMENT '识别码',
  `plan_code` varchar(50) DEFAULT NULL COMMENT '巡检计划识别码',
  `start_date` date DEFAULT NULL COMMENT '开始日期',
  `end_date` date DEFAULT NULL COMMENT '结束日期',
  `start_time` time DEFAULT NULL COMMENT '开始时间',
  `end_time` time DEFAULT NULL COMMENT '结束时间',
  `plan_consumption_time` int(11) DEFAULT NULL COMMENT '预计消耗时间',
  `more_than_time` int(11) DEFAULT NULL COMMENT '可逾时时间',
  `inspection_status` int(11) NOT NULL DEFAULT '0' COMMENT '巡检状态:0未巡检，1巡检中，2已巡检',
  `create_people` varchar(100) DEFAULT NULL COMMENT '创建人',
  `create_date` datetime NOT NULL COMMENT '创建时间',
  `uuid` varchar(50) NOT NULL COMMENT 'uuid',
  `organization_id` int(11) DEFAULT NULL COMMENT '部门id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8 COMMENT='巡检任务';

