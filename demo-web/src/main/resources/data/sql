-- ----------------------------
-- Table structure for pump_trouble_ticket
-- ----------------------------
DROP TABLE IF EXISTS `pump_trouble_ticket`;
CREATE TABLE `pump_trouble_ticket` (
  `id` bigint(19) NOT NULL AUTO_INCREMENT COMMENT '序号',
  `ticket_code` varchar(50) NOT NULL DEFAULT '' COMMENT '故障单识别码',
  `ticket_name` varchar(100) NOT NULL DEFAULT '' COMMENT '故障单名称',
  `plant_code` varchar(50) DEFAULT NULL COMMENT '学习识别码',
  `trouble_code` varchar(50) DEFAULT NULL COMMENT '故障识别码',
  `pump_code` varchar(50) DEFAULT NULL COMMENT '泵识别码',
  `device_code` varchar(50) DEFAULT NULL COMMENT '设备识别码',
  `trouble_describe` varchar(2000) DEFAULT NULL COMMENT '故障描述',
  `submit_people` varchar(100) DEFAULT NULL COMMENT '提交人',
  `submit_time` varchar(100) DEFAULT NULL COMMENT '提交时间',
  `submit_type` int(11) NOT NULL DEFAULT '0' COMMENT '提交类型:0手动提交，1自动提交',
  `create_date` datetime NOT NULL COMMENT '创建时间',
  `uuid` varchar(50) NOT NULL COMMENT 'uuid',
  `organization_id` int(11) DEFAULT NULL COMMENT '运维单位Id',
  `flow_index` tinyint(4) DEFAULT '0' COMMENT '流程索引：0起草，1待受理，2已受理，3已指派，4维修中，5已维修，6结束',
  `organization_client_id` int(11) DEFAULT NULL COMMENT '故障申请单位Id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8 COMMENT='故障单';

